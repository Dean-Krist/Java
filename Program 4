import java.util.Scanner;

public class Program4 {

	public static void main(String[] args) 
	
	{  
		
		Scanner scan= new Scanner(System.in);   
		
		System.out.println("Enter First Number: ");  
		int num1 = scan.nextInt(); 
		
		System.out.println("Enter Second Number: ");  
		int num2 = scan.nextInt();

		System.out.println("Enter third Number: ");  
		int num3 = scan.nextInt();
		
		System.out.println("Enter Fourth Number: ");  
		int num4 = scan.nextInt();
		
		
		
		
			
			
		if (Math.max(num1,num2) > Math.max(num3,num4) ) {
			System.out.println("The maximum number is : " + Math.max(num1,num2));

		}
		
		else if (Math.max(num3,num4) > Math.max(num1,num2) ) {
			System.out.println("The maximum number is : " + Math.max(num3,num4));

		}

		}
}
